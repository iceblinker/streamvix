# Production environment variables
NODE_ENV=production
PORT=7860
PYTHON_BIN=/usr/bin/python3

# Optional: Set these if you want to use MediaFlow proxy
# MEDIAFLOW_PROXY_URL=your_mediaflow_url
# MEDIAFLOW_PROXY_PASSWORD=your_password

# Optional: TMDB API key for better metadata
# TMDB_API_KEY=your_tmdb_api_key

# Optional: Custom EPG URL
# EPG_URL=https://your-custom-epg-url.xml

# Optional: Disable specific providers (set to true to disable)
# DISABLE_VIXSRC=false
# DISABLE_ANIMEUNITY=false
# DISABLE_ANIMESATURN=false
# DISABLE_ANIMEWORLD=false
# DISABLE_GUARDASERIE=false
# DISABLE_GUARDAHD=false
# DISABLE_EUROSTREAMING=false
# DISABLE_LIVE_TV=false
# TVTAP_PROXY_ENABLED=false